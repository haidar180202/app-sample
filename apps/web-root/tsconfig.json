{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "useDefineForClassFields": true,
    "allowImportingTsExtensions": true,
    "noUnusedLocals": false,
    "paths": {
      "*": ["./@mf-types/*"],
      "@public/*": ["./public/*"],
      "@store/*": ["./src/store/*"],
      "@config/*": ["./src/config/*"],
      "@utils/*": ["./src/utils/*"],
      "@components/*": ["./src/components/*"],
      "@pages/*": ["./src/pages/*"],
      "@api/*": ["./src/apis/*"],
      "@const/*": ["./src/const/*"],
      "@style/*": ["./src/style/*"],
      "@helper/*": ["../../utils/*"],
      "@master/*": ["../master/src/apis/*"]
    }
  },
  "include": ["src", "../../utils/decode.ts"]
}
